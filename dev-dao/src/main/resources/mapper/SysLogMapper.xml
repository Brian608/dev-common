<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.feather.mapper.SysLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.feather.domain.SysLog">
        <id column="id" property="id" />
        <result column="user" property="user" />
        <result column="operation" property="operation" />
        <result column="method_type" property="methodType" />
        <result column="uri" property="uri" />
        <result column="time" property="time" />
        <result column="params" property="params" />
        <result column="ip" property="ip" />
        <result column="create_time" property="createTime" />
        <result column="method" property="method" />
        <result column="content_type" property="contentType" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user, operation, method_type, uri, time, params, ip, create_time, method, content_type
    </sql>

</mapper>
